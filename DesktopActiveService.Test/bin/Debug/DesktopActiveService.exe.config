<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="CleanTempPathSttings" type="System.Configuration.NameValueSectionHandler" />
	</configSections>

	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
	</startup>
	<appSettings>
		<!--Wehter execute cleanTempPathService-->
		<add key="ExecuteCleanTempPathService" value="true" />
	</appSettings>

	<CleanTempPathSttings>
		<!--The excution interval in seconds-->
		<add key="ExcutionInterval" value="180" />
		<!--Keep time in seconds-->
		<add key="KeepTime" value="3600" />
	</CleanTempPathSttings>
</configuration>